<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jasperReport name="IndeliveryListRow" resourceBundle="book">
  <parameter class="java.lang.Integer" name="indelivery.number"/>
  <field class="java.lang.String" name="indeliveryrow.product"/>
  <field class="java.lang.String" name="indeliveryrow.descripion"/>
  <field class="java.lang.Integer" name="indeliveryrow.change"/>
  <detail>
    <band height="12" splitType="Stretch">
      <!-- Product number -->
      <element blankWhenNull="true" height="12" kind="textField" width="80" x="80" y="0">
        <expression><![CDATA[$F{indeliveryrow.product}]]></expression>
      </element>
      <!-- Product description -->
      <element blankWhenNull="true" height="12" kind="textField" width="70" x="180" y="0">
        <expression><![CDATA[$F{indeliveryrow.descripion}]]></expression>
      </element>
      <!-- Inpayment sum  -->
      <element blankWhenNull="true" hTextAlign="Right" height="12" kind="textField" pattern="#,##0" width="50" x="480" y="0">
        <expression><![CDATA[$F{indeliveryrow.change}]]></expression>
      </element>
    </band>
  </detail>
</jasperReport>
