<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jasperReport name="InventoryBasis" resourceBundle="book">
  <parameter class="java.awt.Image" name="image.check"/>
  <!-- Declared fields in the report. -->
  <field class="java.lang.String" name="product.number"/>
  <field class="java.lang.String" name="product.description"/>
  <field class="java.lang.Boolean" name="product.expired"/>
  <field class="java.lang.Integer" name="product.stockquantity"/>
  <field class="java.lang.String" name="product.unit"/>
  <field class="java.lang.String" name="product.warehouselocation"/>
  <columnHeader height="15">
    <element height="1" kind="line" width="510" x="20" y="12"/>
    <!-- Column 1 -->
    <element hTextAlign="Left" height="12" kind="textField" width="70" x="20" y="0">
      <expression><![CDATA[$R{inventorybasisreport.column.1}]]></expression>
    </element>
    <element hTextAlign="Left" height="12" kind="textField" width="100" x="90" y="0">
      <expression><![CDATA[$R{inventorybasisreport.column.2}]]></expression>
    </element>
    <element hTextAlign="Center" height="12" kind="textField" width="60" x="240" y="0">
      <expression><![CDATA[$R{inventorybasisreport.column.3}]]></expression>
    </element>
    <element hTextAlign="Left" height="12" kind="textField" width="60" x="300" y="0">
      <expression><![CDATA[$R{inventorybasisreport.column.4}]]></expression>
    </element>
    <element hTextAlign="Right" height="12" kind="textField" width="60" x="360" y="0">
      <expression><![CDATA[$R{inventorybasisreport.column.5}]]></expression>
    </element>
    <element hTextAlign="Right" height="12" kind="textField" width="80" x="450" y="0">
      <expression><![CDATA[$R{inventorybasisreport.column.6}]]></expression>
    </element>
  </columnHeader>
  <detail>
    <band height="14">
      <element blankWhenNull="true" height="12" kind="textField" width="70" x="20" y="0">
        <expression><![CDATA[$F{product.number}]]></expression>
      </element>
      <element blankWhenNull="true" height="12" kind="textField" width="100" x="90" y="0">
        <expression><![CDATA[$F{product.description}]]></expression>
      </element>
      <element blankWhenNull="false" hTextAlign="Center" height="12" kind="textField" width="60" x="240" y="0">
        <expression><![CDATA[$F{product.expired}.booleanValue() ? $R{inventorybasisreport.expired} : ""]]></expression>
      </element>
      <element hImageAlign="Center" height="12" kind="image" onErrorType="Icon" vImageAlign="Middle" width="60" x="240" y="0">
        <expression><![CDATA[$P{image.check}]]></expression>
      </element>
      <element blankWhenNull="false" hTextAlign="Left" height="12" kind="textField" width="60" x="300" y="0">
        <expression><![CDATA[$F{product.warehouselocation}]]></expression>
      </element>
      <element blankWhenNull="false" hTextAlign="Right" height="12" kind="textField" width="60" x="360" y="0">
        <expression><![CDATA[$F{product.stockquantity}]]></expression>
      </element>
      <element blankWhenNull="true" hTextAlign="Left" height="12" kind="textField" width="29" x="421" y="0">
        <expression><![CDATA[$F{product.unit}]]></expression>
      </element>
      <element blankWhenNull="true" hTextAlign="Right" height="12" kind="textField" width="80" x="450" y="0">
        <expression><![CDATA[""]]></expression>
      </element>
    </band>
  </detail>
</jasperReport>
