<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jasperReport name="Customers" resourceBundle="book">
  <!-- Declared fields in the report. -->
  <field class="java.lang.String" name="customer.number"/>
  <field class="java.lang.String" name="customer.name"/>
  <field class="java.lang.String" name="customer.contact"/>
  <field class="java.lang.String" name="customer.registrationnumber"/>
  <field class="java.lang.String" name="customer.phone"/>
  <columnHeader height="15">
    <element height="1" kind="line" width="510" x="20" y="12"/>
    <!-- Column 1 -->
    <element hTextAlign="Left" height="12" kind="textField" width="60" x="20" y="0">
      <expression><![CDATA[$R{customerlistreport.column.1}]]></expression>
    </element>
    <element hTextAlign="Left" height="12" kind="textField" width="150" x="80" y="0">
      <expression><![CDATA[$R{customerlistreport.column.2}]]></expression>
    </element>
    <element height="12" kind="textField" width="100" x="230" y="0">
      <expression><![CDATA[$R{customerlistreport.column.3}]]></expression>
    </element>
    <element height="12" kind="textField" width="100" x="330" y="0">
      <expression><![CDATA[$R{customerlistreport.column.4}]]></expression>
    </element>
    <element hTextAlign="Right" height="12" kind="textField" width="100" x="430" y="0">
      <expression><![CDATA[$R{customerlistreport.column.5}]]></expression>
    </element>
  </columnHeader>
  <detail>
    <band height="14" splitType="Stretch">
      <!-- Number-->
      <element blankWhenNull="true" height="14" kind="textField" width="60" x="20" y="0">
        <expression><![CDATA[$F{customer.number}]]></expression>
      </element>
      <!-- Name-->
      <element blankWhenNull="true" height="14" kind="textField" width="150" x="80" y="0">
        <expression><![CDATA[$F{customer.name}]]></expression>
      </element>
      <!-- Our contact person-->
      <element blankWhenNull="true" height="12" kind="textField" width="100" x="230" y="0">
        <expression><![CDATA[$F{customer.contact}]]></expression>
      </element>
      <!-- Registration number -->
      <element blankWhenNull="true" height="12" kind="textField" width="100" x="330" y="0">
        <expression><![CDATA[$F{customer.registrationnumber}]]></expression>
      </element>
      <!-- Phone -->
      <element blankWhenNull="true" hTextAlign="Right" height="12" kind="textField" width="100" x="430" y="0">
        <expression><![CDATA[$F{customer.phone}]]></expression>
      </element>
    </band>
  </detail>
</jasperReport>
