<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jasperReport name="MainBook" resourceBundle="book">
  <parameter class="net.sf.jasperreports.engine.JasperReport" name="Report"/>
  <parameter class="java.util.HashMap" name="Parameters"/>
  <!-- Declared fields in the report. -->
  <field class="java.lang.String" name="project.number"/>
  <field class="java.lang.String" name="project.description"/>
  <field class="net.sf.jasperreports.engine.JRDataSource" name="month.data"/>
  <columnHeader height="30">
    <element height="1" kind="line" width="510" x="20" y="23"/>
    <!-- Column 1 -->
    <element height="24" kind="textField" width="80" x="20" y="0">
      <expression><![CDATA[$R{projectrevenue.column.1}]]></expression>
    </element>
    <element height="24" kind="textField" width="220" x="100" y="0">
      <expression><![CDATA[$R{projectrevenue.column.2}]]></expression>
    </element>
    <element hTextAlign="Right" height="24" kind="textField" width="80" x="450" y="0">
      <expression><![CDATA[$R{projectrevenue.column.3}]]></expression>
    </element>
  </columnHeader>
  <detail>
    <band height="30" splitType="Stretch">
      <!-- Title -->
      <element height="14" kind="textField" width="80" x="20" y="0">
        <expression><![CDATA[$F{project.number}]]></expression>
      </element>
      <element height="14" kind="textField" width="250" x="100" y="0">
        <expression><![CDATA[$F{project.description}]]></expression>
      </element>
      <element height="0" kind="subreport" usingCache="false" width="555" x="20" y="14">
        <parametersMapExpression><![CDATA[new java.util.HashMap() {{ put("project.number", $F{project.number}); }}]]></parametersMapExpression>
        <dataSourceExpression><![CDATA[$F{month.data}]]></dataSourceExpression>
        <expression><![CDATA[$P{Report}]]></expression>
      </element>
    </band>
  </detail>
</jasperReport>
