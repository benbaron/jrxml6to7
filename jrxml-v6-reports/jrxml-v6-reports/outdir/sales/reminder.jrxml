<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jasperReport name="Invoice">
  <!--- Declared parameters in the report. -->
  <parameter class="java.lang.String" name="title"/>
  <parameter class="java.awt.Image" name="company.logo"/>
  <parameter class="java.lang.String" name="company.name"/>
  <parameter class="java.lang.String" name="company.address1"/>
  <parameter class="java.lang.String" name="company.address2"/>
  <parameter class="java.lang.String" name="company.zipcode"/>
  <parameter class="java.lang.String" name="company.city"/>
  <parameter class="java.lang.String" name="company.country"/>
  <parameter class="java.lang.String" name="company.phone"/>
  <parameter class="java.lang.String" name="company.telefax"/>
  <parameter class="java.lang.String" name="company.residence"/>
  <parameter class="java.lang.String" name="company.email"/>
  <parameter class="java.lang.String" name="company.homepage"/>
  <parameter class="java.math.BigDecimal" name="company.reminderfee"/>
  <parameter class="java.math.BigDecimal" name="company.delayinterest"/>
  <parameter class="java.lang.String" name="company.plusaccount"/>
  <parameter class="java.lang.String" name="company.bankaccount"/>
  <parameter class="java.lang.String" name="company.bic"/>
  <parameter class="java.lang.String" name="company.iban"/>
  <parameter class="java.lang.String" name="company.bank"/>
  <parameter class="java.lang.Integer" name="number"/>
  <parameter class="java.lang.String" name="date"/>
  <parameter class="java.lang.String" name="text"/>
  <parameter class="java.lang.String" name="customer.invoiceadress.name"/>
  <parameter class="java.lang.String" name="customer.invoiceadress.address1"/>
  <parameter class="java.lang.String" name="customer.invoiceadress.address2"/>
  <parameter class="java.lang.String" name="customer.invoiceadress.zipcode"/>
  <parameter class="java.lang.String" name="customer.invoiceadress.city"/>
  <parameter class="java.lang.String" name="customer.invoiceadress.country"/>
  <parameter class="java.lang.String" name="customer.customernr"/>
  <parameter class="java.lang.String" name="customer.currency"/>
  <parameter class="java.lang.String" name="customer.yourcontact"/>
  <parameter class="java.lang.String" name="customer.ourcontact"/>
  <parameter class="java.math.BigDecimal" name="reminder.reminderfee"/>
  <parameter class="net.sf.jasperreports.engine.JasperReport" name="subreport.report"/>
  <field class="net.sf.jasperreports.engine.JRDataSource" name="subreport.datasource"/>
  <variable calculation="System" class="java.math.BigDecimal" name="delayfee.sum" resetType="Report">
    <initialValueExpression><![CDATA[new BigDecimal(0.0)]]></initialValueExpression>
  </variable>
  <variable calculation="System" class="java.math.BigDecimal" name="saldo.sum" resetType="Report">
    <initialValueExpression><![CDATA[new BigDecimal(0.0)]]></initialValueExpression>
  </variable>
  <columnHeader height="128">
    <!-- invoice adress frame -->
    <element height="86" kind="frame" width="250" x="300" y="0">
      <box>
        <pen lineColor="#000000" lineWidth="0.5"/>
      </box>
      <element blankWhenNull="true" hTextAlign="Left" height="14" kind="textField" width="240" x="5" y="2">
        <expression><![CDATA[$R{reminderreport.invoiceadress}]]></expression>
      </element>
      <element blankWhenNull="true" hTextAlign="Left" height="14" kind="textField" width="240" x="10" y="14">
        <expression><![CDATA[$P{customer.invoiceadress.name}]]></expression>
      </element>
      <element blankWhenNull="true" hTextAlign="Left" height="14" kind="textField" width="240" x="10" y="28">
        <expression><![CDATA[$P{customer.invoiceadress.address1}]]></expression>
      </element>
      <element blankWhenNull="true" hTextAlign="Left" height="14" kind="textField" width="240" x="10" y="42">
        <expression><![CDATA[$P{customer.invoiceadress.address2}]]></expression>
      </element>
      <element blankWhenNull="true" hTextAlign="Left" height="14" kind="textField" width="240" x="10" y="56">
        <expression><![CDATA[$P{customer.invoiceadress.zipcode} + " " + $P{customer.invoiceadress.city}]]></expression>
      </element>
      <element blankWhenNull="true" hTextAlign="Left" height="14" kind="textField" width="240" x="10" y="70">
        <expression><![CDATA[$P{customer.invoiceadress.country}]]></expression>
      </element>
    </element>
    <!-- Invoice details, Column 1 -->
    <element height="30" kind="frame" width="250" x="45" y="90">
      <element blankWhenNull="false" hTextAlign="Left" height="12" kind="textField" width="100" x="0" y="0">
        <expression><![CDATA[$R{customer.ourcontact}]]></expression>
      </element>
      <element blankWhenNull="true" hTextAlign="Left" height="12" kind="textField" width="150" x="100" y="0">
        <expression><![CDATA[$P{customer.ourcontact}]]></expression>
      </element>
      <element blankWhenNull="false" hTextAlign="Left" height="12" kind="textField" width="100" x="0" y="15">
        <expression><![CDATA[$R{company.reminderfee}]]></expression>
      </element>
      <element blankWhenNull="true" hTextAlign="Left" height="12" kind="textField" pattern="#,##0.00" width="150" x="100" y="15">
        <expression><![CDATA[$P{company.reminderfee}]]></expression>
      </element>
    </element>
    <!-- Invoice details, Column 2 -->
    <element height="30" kind="frame" width="250" x="305" y="90">
      <element blankWhenNull="false" hTextAlign="Left" height="12" kind="textField" width="100" x="0" y="0">
        <expression><![CDATA[$R{customer.customernr}]]></expression>
      </element>
      <element blankWhenNull="true" hTextAlign="Left" height="12" kind="textField" width="150" x="100" y="0">
        <expression><![CDATA[$P{customer.customernr}]]></expression>
      </element>
      <element blankWhenNull="false" hTextAlign="Left" height="12" kind="textField" width="100" x="0" y="15">
        <expression><![CDATA[$R{customer.yourcontact}]]></expression>
      </element>
      <element blankWhenNull="true" hTextAlign="Left" height="12" kind="textField" width="150" x="100" y="15">
        <expression><![CDATA[$P{customer.yourcontact}]]></expression>
      </element>
    </element>
  </columnHeader>
  <detail>
    <band height="484" splitType="Stretch">
      <!-- Subreport for the rows 385-->
      <element height="459" kind="frame" width="510" x="40" y="0">
        <box>
          <pen lineColor="#000000" lineWidth="0.5"/>
        </box>
        <element height="459" kind="subreport" usingCache="false" width="510" x="0" y="0">
          <parametersMapExpression><![CDATA[new java.util.HashMap(($P{REPORT_PARAMETERS_MAP}) == null ? java.util.Collections.emptyMap() : ($P{REPORT_PARAMETERS_MAP})) {{ put("REPORT_RESOURCE_BUNDLE", $P{REPORT_RESOURCE_BUNDLE}); }}]]></parametersMapExpression>
          <dataSourceExpression><![CDATA[$F{subreport.datasource}]]></dataSourceExpression>
          <expression><![CDATA[$P{subreport.report}]]></expression>
          <returnValue subreportVariable="delayfee.sum" toVariable="delayfee.sum"/>
          <returnValue subreportVariable="saldo.sum" toVariable="saldo.sum"/>
        </element>
      </element>
      <!-- Summary fields -->
      <element height="25" kind="frame" width="510" x="40" y="459">
        <box>
          <pen lineColor="#000000" lineWidth="0.5"/>
        </box>
        <element height="25" kind="line" width="1" x="160" y="0"/>
        <element blankWhenNull="true" hTextAlign="Left" height="12" kind="textField" width="177" x="163" y="1">
          <expression><![CDATA[$R{reminderreport.totalreminderfee}]]></expression>
        </element>
        <element blankWhenNull="true" hTextAlign="Right" height="14" kind="textField" pattern="#,##0.00" width="77" x="160" y="10">
          <expression><![CDATA[$P{reminder.reminderfee}]]></expression>
        </element>
        <element height="25" kind="line" width="1" x="240" y="0"/>
        <element blankWhenNull="true" hTextAlign="Left" height="12" kind="textField" width="177" x="243" y="1">
          <expression><![CDATA[$R{reminderreport.delayfee}]]></expression>
        </element>
        <element blankWhenNull="true" evaluationTime="Report" hTextAlign="Right" height="14" kind="textField" pattern="#,##0.00" width="77" x="240" y="10">
          <expression><![CDATA[$V{delayfee.sum} == null ? new BigDecimal(0.0): $V{delayfee.sum}]]></expression>
        </element>
        <element height="25" kind="line" width="1" x="320" y="0"/>
        <element blankWhenNull="true" hTextAlign="Left" height="12" kind="textField" width="177" x="323" y="1">
          <expression><![CDATA[$R{reminderreport.saldosum}]]></expression>
        </element>
        <element blankWhenNull="true" evaluationTime="Report" hTextAlign="Right" height="14" kind="textField" pattern="#,##0.00" width="77" x="320" y="10">
          <expression><![CDATA[$V{saldo.sum} == null ? new BigDecimal(0.0): $V{saldo.sum}]]></expression>
        </element>
        <element height="25" kind="line" width="1" x="400" y="0"/>
        <element blankWhenNull="true" evaluationTime="Report" hTextAlign="Left" height="12" kind="textField" width="80" x="403" y="1">
          <expression><![CDATA[$R{reminderreport.totalsum}]]></expression>
        </element>
        <element blankWhenNull="true" evaluationTime="Report" hTextAlign="Right" height="14" kind="textField" pattern="#,##0.00" width="77" x="400" y="10">
          <expression><![CDATA[$V{delayfee.sum}.add($V{saldo.sum}).add($P{reminder.reminderfee})]]></expression>
        </element>
        <element blankWhenNull="true" hTextAlign="Left" height="14" kind="textField" pattern="#,##0.00" width="27" x="483" y="10">
          <expression><![CDATA[$P{customer.currency}]]></expression>
        </element>
      </element>
    </band>
  </detail>
</jasperReport>
