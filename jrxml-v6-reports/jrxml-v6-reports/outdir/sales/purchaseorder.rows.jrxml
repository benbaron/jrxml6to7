<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jasperReport bottomMargin="0" leftMargin="0" name="PurchaseOrderRows" rightMargin="0" topMargin="0">
  <field class="java.lang.String" name="row.supplierarticlenr"/>
  <field class="java.lang.String" name="row.number"/>
  <field class="java.lang.String" name="row.description"/>
  <field class="java.lang.Integer" name="row.quantity"/>
  <field class="java.lang.String" name="row.unit"/>
  <field class="java.math.BigDecimal" name="row.unitprice"/>
  <field class="java.math.BigDecimal" name="row.sum"/>
  <columnHeader height="16">
    <element height="1" kind="line" width="510" x="0" y="0"/>
    <element height="1" kind="line" width="510" x="0" y="13"/>
    <!-- Supplier article number -->
    <element blankWhenNull="true" hTextAlign="Left" height="12" kind="textField" width="70" x="10" y="2">
      <expression><![CDATA[$R{purchaseorderreport.column.1}]]></expression>
    </element>
    <!-- Product number -->
    <element hTextAlign="Left" height="12" kind="textField" width="70" x="80" y="2">
      <expression><![CDATA[$R{purchaseorderreport.column.2}]]></expression>
    </element>
    <!-- Description -->
    <element blankWhenNull="true" hTextAlign="Left" height="12" kind="textField" width="90" x="150" y="2">
      <expression><![CDATA[$R{purchaseorderreport.column.3}]]></expression>
    </element>
    <!-- Unitprice -->
    <element blankWhenNull="true" hTextAlign="Right" height="12" kind="textField" width="70" x="300" y="2">
      <expression><![CDATA[$R{purchaseorderreport.column.4}]]></expression>
    </element>
    <!-- Count -->
    <element blankWhenNull="true" hTextAlign="Right" height="12" kind="textField" width="70" x="360" y="2">
      <expression><![CDATA[$R{purchaseorderreport.column.5}]]></expression>
    </element>
    <!-- Sum -->
    <element blankWhenNull="true" hTextAlign="Right" height="12" kind="textField" width="50" x="450" y="2">
      <expression><![CDATA[$R{purchaseorderreport.column.6}]]></expression>
    </element>
  </columnHeader>
  <detail>
    <band height="12" splitType="Stretch">
      <!-- Supplier article number -->
      <element blankWhenNull="true" height="12" kind="textField" width="70" x="10" y="0">
        <expression><![CDATA[$F{row.supplierarticlenr}]]></expression>
      </element>
      <!-- Number -->
      <element blankWhenNull="true" height="12" kind="textField" width="70" x="80" y="0">
        <expression><![CDATA[$F{row.number}]]></expression>
      </element>
      <!-- Description -->
      <element blankWhenNull="true" height="12" kind="textField" width="250" x="150" y="0">
        <expression><![CDATA[$F{row.description}]]></expression>
      </element>
      <!-- Unitprice -->
      <element blankWhenNull="true" hTextAlign="Right" height="12" kind="textField" pattern="#,##0.00" width="70" x="300" y="0">
        <expression><![CDATA[$F{row.unitprice}]]></expression>
      </element>
      <!-- Count -->
      <element blankWhenNull="true" hTextAlign="Right" height="12" kind="textField" width="70" x="360" y="0">
        <expression><![CDATA[$F{row.quantity}]]></expression>
      </element>
      <!-- Unit -->
      <element blankWhenNull="true" hTextAlign="Left" height="12" kind="textField" width="18" x="432" y="0">
        <expression><![CDATA[$F{row.unit}]]></expression>
      </element>
      <!-- Row sum  -->
      <element blankWhenNull="true" hTextAlign="Right" height="12" kind="textField" pattern="#,##0.00" width="50" x="450" y="0">
        <expression><![CDATA[$F{row.sum}]]></expression>
      </element>
    </band>
  </detail>
  <pageFooter height="14">
    <!-- Number -->
    <element hTextAlign="Right" height="14" kind="textField" vTextAlign="Bottom" width="510" x="0" y="0">
      <expression><![CDATA[$R{purchaseorderreport.continuing}]]></expression>
    </element>
  </pageFooter>
  <lastPageFooter height="0"/>
</jasperReport>
